{
    "manifest_version": 3,
    "name": "Greg's Movie Player",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "pause.jpg"
    }, 
    "content_scripts": [
        {
        "js": ["scripts/content.js"],
        "matches": [
          "https://www.imdb.com/title/*"
        ]
        }
    ],
    "host_permissions": [
      "*://vidsrc.xyz/embed/*"
    ],
    "background": {
      "service_worker": "service-worker.js"
    }
    
}